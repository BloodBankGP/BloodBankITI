@model BloodBankITI.Models.NGO

@{
    ViewBag.Title = "title";
    Layout = "~/Views/Shared/_Layout2.cshtml";
}
<link href="~/Content/materialize.css" rel="stylesheet" />

<div class="render">
    <div class="row">
        <div class="" style="border:hidden;">
            <table class="table table-user-information">
                <tbody>
                    <tr>
                        <td>@Model.Name</td>
                        <td>الاسم </td>
                    </tr>
                    <tr>
                        <td>@Model.Address</td>
                        <td>العنوان</td>
                    </tr>
                    <tr>
                        <td>@Model.Phone</td>
                        <td>رقم التلفون</td>
                    </tr>
                    <tr>
                        <td>@Model.City</td>
                        <td>المدينة</td>
                    </tr>
                    <tr>
                        <td>@Model.Website</td>
                        <td>البريد الإلكتروني</td>
                    </tr>
                    <tr>
                        <td>@Model.Fb</td>
                        <td>الفيس بوك</td>
                    </tr>
                </tbody>
            </table>
            <center>
                <p style="text-align:center;">@Html.ActionLink("العودة للجمعيات الخيرية", "NGOs", null, new { @class = "btn waves-effect waves-light red darken-4" })</p>
            </center>
        </div>


    </div>
</div>

@model List<BloodBankITI.Models.NGO_selectt_Result>
@{
    ViewBag.Title = "Ngo";
    Layout = "~/Views/Shared/_Layout2.cshtml";
}
<link href="~/Content/materialize.css" rel="stylesheet" />
<link href="~/Content/bootstrap.css" rel="stylesheet" />

<div class="render">
    <h3 style="font-family:Myriad عربي;">الجمعيات الخيرية</h3>
    <hr />

    <div class="table-responsive">
        <table class="table table-striped" style="background-color:wheat;margin-right:inherit; width:800px;">
            <tr>
                <th > حذف</th>
                <th >تعديل</th>

                <th>الحالة</th>
                <th >الموافقة</th>
                <th >العنوان</th>
                <th >رقم التلفون</th>
                <th>المدينة</th>
                <th >الإسم</th>
                <th >الرقم المسلسل</th>
  
            </tr>

            @foreach (var a in Model)
            {
                <tr>
                    <td> @Html.ActionLink("حذف", "NGODelete", new { id = a.NID })</td>
                    <td> @Html.ActionLink("تعديل", "NGOEDIT", new { id = a.NID })</td>

                    <td>@(a.Status) </td>
                    <td>@(a.Approved) </td>
                    <td>@(a.Address) </td>
                    <td>@(a.Phone) </td>
                    <td>@(a.CityName) </td>
                    <td>@(a.Name) </td>
                    <td>@(a.NID) </td>
                </tr>

            }

        </table>
        </div>
   
       
        <p style="text-align:center;">@Html.ActionLink("العودة للصفحة الرئيسية", "Index",null, new { @class = "btn waves-effect waves-light red darken-4" })</p>
    
        
    </div>






                
           

            
    

    
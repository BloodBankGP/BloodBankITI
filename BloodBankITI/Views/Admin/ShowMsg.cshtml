@model BloodBankITI.Models.GetContact_Result

@{
    ViewBag.Title = "title";
    Layout = "~/Views/Shared/_Layout2.cshtml";
}
<link href="~/Content/materialize.css" rel="stylesheet" />

    <div class="render">
        <div class="row">
            <div class="" style="border:hidden;">
                <table class="table table-user-information">
                    <tbody>
                        <tr>
                            <td>@Model.FName</td>
                            <td>الاسم الأول</td>
                        </tr>
                        <tr>
                            <td>@Model.LName</td>
                            <td>الاسم الأخير</td>
                        </tr>
                        <tr>
                            <td>@Model.Age</td>
                            <td>العمر</td>
                        </tr>
                        <tr>
                            <td>@Model.City</td>
                            <td>المدينة</td>
                        </tr>
                        <tr>
                            <td>@Model.Email</td>
                            <td>البريد الإلكتروني</td>
                        </tr>
                        <tr>
                            <td><bre>@Model.Msg</bre></td>
                            <td>الرسالة</td>
                        </tr>
                    </tbody>
                </table>
                <center >
                    @Html.ActionLink("العودة لصفحة الرسائل", "ShowMsgs", null, new { @class = "btn waves-effect waves-light red darken-4" })
                </center>
            </div>
           

        </div>
    </div>



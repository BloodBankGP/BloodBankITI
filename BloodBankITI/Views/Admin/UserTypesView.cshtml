@model List<BloodBankITI.Models.UserType_selectt_Result>
@{
    ViewBag.Title = "UserTypesView";
    Layout = "~/Views/Shared/_Layout2.cshtml";
}

<div class="render">
    <h2>UserTypesView</h2>
    @Html.ActionLink("ادخال مستخدم جديد", "UserTypesInsert")
<br />

    <table class="table table-bordered table-hover table-striped">
        <tr>
            <th>النوع </th>
            <th> الرقم المسلسل  </th>
            <th> تعديل  </th>


        </tr>

        @foreach (var a in Model)
        {
            <tr>
                <td>@(a.Type) </td>
                <td>@(a.UTID) </td>
                <td> @Html.ActionLink("تعديل", "UserTypesEdit", new { id = a.UTID })</td>




            </tr>

        }

    </table>


    @Html.ActionLink("الرجوع للصفحة الرئيسية", "Index")


</div>